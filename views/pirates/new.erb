<h1>Welcome to the Nested Forms Lab!</h1>
<form action="/pirates" method="post">
  <h1>Pirate</h1>
  <label for="pirate[name]">Name:</label>
  <input type="text" id="pirate[name]" name="pirate[name]" value="">
  <label for="pirate[weight]">Weight:</label>
  <input type="text" id="pirate[weight]" name="pirate[weight]" value="">
  <label for="pirate[height]">Height:</label>
  <input type="text" id="pirate[height]" name="pirate[height]" value="">

  <h2>Ship 1</h2>
  <label for="pirate[ships][][name]">Ship Name:</label>
  <input type="text" id="ship_name_1" name="pirate[ships][][name]" value="">
  <label for="pirate[ships][][type]">Ship Type:</label>
  <input type="text" id="ship_type_1" name="pirate[ships][][type]" value="">
  <label for="pirate[ships][][booty]">Ship Booty:</label>
  <input type="text" id="ship_booty_1" name="pirate[ships][][booty]" value="">

  <h2>Ship 2</h2>
  <label for="pirate[ships][][name]">Ship Name:</label>
  <input type="text" id="ship_name_2" name="pirate[ships][][name]" value="">
  <label for="pirate[ships][][type]">Ship Type:</label>
  <input type="text" id="ship_type_2" name="pirate[ships][][type]" value="">
  <label for="pirate[ships][][booty]">Ship Booty:</label>
  <input type="text" id="ship_booty_2" name="pirate[ships][][booty]" value="">

  <input type="submit" name="" value="Submit">
</form>
